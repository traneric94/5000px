<!DOCTYPE html>
<html>
  <head>
    <title>PxProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <script type="text/javascript">
      window.logo = "<%= image_url('logo.png') %>"
      window.profile_pic = "<%= image_url('userpic.png') %>"
      window.background = "<%= image_url('newbackground.jpg') %>"
    </script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
